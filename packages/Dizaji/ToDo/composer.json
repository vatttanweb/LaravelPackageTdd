{
    "name": "dizaji/todo",
    "description": "This package use tdd to create todo list package",
    "autoload": {
        "psr-4": {
            "Dizaji\\ToDo\\": "src/",
            "Dizaji\\ToDo\\Tests\\": "tests/"
        },
        "classmap": [
            "src/Migrations"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dizaji\\ToDo\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Akbar Dizaji"
        }
    ],
    "extra": {
        "laravel": {
            "providers":[
                "Dizaji\\ToDo\\src\\ToDoServiceProvider"
            ]
        }
    },
    "require": {
        "phpunit/phpunit": "^9.5"
    },
    "require-dev": {
        "orchestra/testbench": "^6.24"
    }
}
